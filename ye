game.Players.LocalPlayer.PlayerGui.ChildAdded:Connect(function()
for i,v in pairs(game.Players.LocalPlayer.PlayerGui.LockpickGUI:GetDescendants()) do
    if v.Name == "UIScale" then
        v.Scale = 10
    end
    end

end)
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "criminality shitty script", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local Tab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
Tab:AddButton({
	Name = "Medium safe tp",
	Callback = function()
      		getgenv().ok = true

for i,v in ipairs(game.Workspace.Map.BredMakurz:GetChildren()) do
if v.Name:match('MediumSafe') and v.Values.Broken.Value == false then

while getgenv().ok do wait()
    
spawn( function()
wait(12)
getgenv().ok = false
end)

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parts.Door.CFrame

local args = {
    [1] = "__--r",
    [2] = game.Players.LocalPlayer.Character.HumanoidRootPart.Position,
    [3] = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
}

game:GetService("ReplicatedStorage").Events.__DFfDD:FireServer(unpack(args))

end
   end
end
  	end    
})
